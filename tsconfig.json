{
    "compilerOptions": {
      "outDir": "./dist",
      "declaration": true,
      "declarationMap": true,
      "module": "ESNext",
      "target": "ES2019",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
  }